<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>neo-build</AssemblyName>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageTags>NEO;Blockchain;Build;Core;CLI;NEBuild</PackageTags>
    <PackAsTool>True</PackAsTool>
    <ApplicationIcon>../../../.neo/icons/neo-build.ico</ApplicationIcon>
    <ToolCommandName>nebuild</ToolCommandName>
    <RollForward>LatestMinor</RollForward>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../../../.neo/icons/neo-build.ico" Visible="true" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Neo.Build.Core\Neo.Build.Core.csproj" />
  </ItemGroup>

</Project>
